// Declaración y uso de arrays

fn main() {
    println("=== Test Arrays Básicos ===")
    
    // Declarar array con valores
    let arr = [10, 20, 30, 40, 50]
    
    println("Array creado: [10, 20, 30, 40, 50]")
    
    // Acceder a elementos por índice
    let primero = arr[0]
    print("Primer elemento (arr[0]): ")
    println(primero)
    
    let tercero = arr[2]
    print("Tercer elemento (arr[2]): ")
    println(tercero)
    
    println("Test de arrays completado!")
}
